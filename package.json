{
  "name": "bridge-frontend",
  "version": "1.2.0",
  "description": "Bridge frontend to transfer ERC20 tokens between EVM networks",
  "main": "index.js",
  "keywords": [
    "react",
    "reactjs",
    "dashboard",
    "dashboard-react"
  ],
  "author": "JadenX",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
    "map:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true",
    "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^6.1.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@fortawesome/sharp-solid-svg-icons": "^6.2.0",
    "@types/node": "^16.4.0",
    "@types/react": "^17.0.20",
    "@web3-react/core": "8.0.35-beta.0",
    "@web3-react/metamask": "8.0.28-beta.0",
    "@web3-react/network": "8.0.27-beta.0",
    "@web3-react/types": "8.0.20-beta.0",
    "bootstrap": "4.6.0",
    "ethers": "^5.7.2",
    "list.js": "^2.3.1",
    "node-sass": "^4.14.1",
    "react": "17.0.1",
    "react-chartjs-2": "2.11.1",
    "react-datepicker": "^4.8.0",
    "react-dom": "17.0.1",
    "react-json-view": "^1.21.3",
    "react-quill": "^1.3.5",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "react-truncate-markup": "^5.1.0",
    "reactstrap": "8.9.0"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@types/list.js": "^2.3.1",
    "@types/react-datepicker": "^4.4.2",
    "@types/react-router-dom": "^5.1.8"
  },
  "optionalDependencies": {
    "eslint-plugin-flowtype": "5.2.0",
    "jquery": "3.5.1",
    "typescript": "^4.1.3"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  }
}
